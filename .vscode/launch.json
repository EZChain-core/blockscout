{
    "version": "2.0.0",
    "configurations": [
        
        {
            "type": "mix_task",
            "name": "mix deps.compile",
            "request": "launch",
            "task": "deps.compile",
            "taskArgs": [],
            "projectDir": "${workspaceRoot}",
            "env": {
            },
            "console": "integratedTerminal"
        },
        {
            "type": "mix_task",
            "name": "mix phx.server",
            "request": "launch",
            "taskArgs": [],
            "projectDir": "${workspaceRoot}",
            "env": {
                // "ETHEREUM_JSONRPC_HTTP_URL": "http://127.0.0.1:9650/ext/bc/C/rpc",
                "ETHEREUM_JSONRPC_HTTP_URL": "http://127.0.0.1:8545",
                "ETHEREUM_JSONRPC_VARIANT": "ganache",
                "ETHEREUM_JSONRPC_TRANSPORT": "http",
                "DB_HOST": "127.0.0.1",
                "DB_PORT": "5432",
                "DB_USERNAME": "postgress",
                "BLOCKSCOUT_PROTOCOL": "http",
                "DATABASE_URL": "postgresql://postgres:@127.0.0.1:5432/explorer?ssl=false",
                "DB_URL": "postgresql://postgres:@127.0.0.1:5432/explorer?ssl=false",
                "PORT": "4000",
                "MIX_ENV": "prod",
                "CHAIN_SPEC_PATH": "/",
                "FIRST_BLOCK": "0"
            },
            "excludeModules": ["ExKeccak", "Bcrypt.Base", ":prometheus_process_collector", ],
            "cwd": "${workspaceRoot}",
            "outputCapture": "std",
        },
        {
            "type": "mix_task",
            "name": "mix debug",
            "request": "launch",
            "task": "test",
            "taskArgs": [
                "--trace"
            ],
            "startApps": true,
            "projectDir": "${workspaceRoot}",
            "requireFiles": [
                "test/**/test_helper.exs",
                "test/**/*_test.exs"
            ]
        }
    ]
}